code:  import.lean {src/code}
import {unitt}

do ::

unix?: false
win?: true

test "are equal" [
    assert -> 1 = code\fun
]

test.skip: unix? "are equal" [
    assert -> 1 = code\fun
]

test.skip: win? "are equal" [
    assert -> 1 = code\fun
]


test "are different" [
    assert -> 2 <> code\fun
    assert -> 3 <> code\fun
]

test "will pass" [
    assert -> 1 = code\fun
]

test "will fail" [
    assert -> 2 = code\fun
]